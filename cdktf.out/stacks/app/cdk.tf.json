{
  "//": {
    "metadata": {
      "backend": "local",
      "stackName": "app",
      "version": "0.12.0"
    },
    "outputs": {}
  },
  "provider": {
    "kubernetes": [
      {}
    ]
  },
  "resource": {
    "kubernetes_deployment": {
      "deployment_myapp": {
        "//": {
          "metadata": {
            "path": "app/deployment_myapp",
            "uniqueId": "deployment_myapp"
          }
        },
        "metadata": {
          "labels": {
            "app": "myapp",
            "component": "frontend",
            "environment": "dev"
          },
          "name": "myapp",
          "namespace": "dev"
        },
        "spec": {
          "replicas": "1",
          "selector": {
            "match_labels": {
              "app": "myapp",
              "component": "frontend",
              "environment": "dev"
            }
          },
          "template": {
            "metadata": {
              "labels": {
                "app": "myapp",
                "component": "frontend",
                "environment": "dev"
              }
            },
            "spec": {
              "container": [
                {
                  "image": "nginx:latest",
                  "name": "frontend"
                }
              ]
            }
          }
        }
      }
    },
    "kubernetes_manifest": {
      "app_tf": {
        "//": {
          "metadata": {
            "path": "app/app_tf",
            "uniqueId": "app_tf"
          }
        },
        "manifest": {
          "apiVersion": "infra.contrib.fluxcd.io/v1alpha1",
          "kind": "Terraform",
          "metadata": {
            "name": "app-tf",
            "namespace": "dev"
          },
          "spec": {
            "approvePlan": "auto",
            "interval": "1m",
            "path": "./cdktf.out/stacks/app/",
            "sourceRef": {
              "kind": "GitRepository",
              "name": "cdktf-app",
              "namespace": "dev"
            }
          }
        }
      },
      "cdktf_app": {
        "//": {
          "metadata": {
            "path": "app/cdktf_app",
            "uniqueId": "cdktf_app"
          }
        },
        "manifest": {
          "apiVersion": "source.toolkit.fluxcd.io/v1beta2",
          "kind": "GitRepository",
          "metadata": {
            "name": "cdktf-app",
            "namespace": "dev"
          },
          "spec": {
            "interval": "30s",
            "ref": {
              "branch": "main"
            },
            "url": "https://github.com/chanwit/cdktf-app"
          }
        }
      }
    }
  },
  "terraform": {
    "required_providers": {
      "kubernetes": {
        "source": "kubernetes",
        "version": "2.12.1"
      }
    }
  }
}
